{
  "name": "royal-thing",
  "version": "4.5.0",
  "description": "A process that restarts the local registrant when needed",
  "main": "README.js",
  "scripts": {
    "prepublish": "./node_modules/.bin/coffee -c *.coffee.md",
    "pretest": "npm install",
    "test": "mocha",
    "start": "daemon -n royal-thing -o daemon.debug -D \"$PWD\" -r -- node ./README.js",
    "stop": "daemon -n royal-thing -o daemon.debug --stop"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/shimaore/royal-thing"
  },
  "keywords": [
    "CCNQ",
    "registrant"
  ],
  "author": "St√©phane Alnet <stephane@shimaore.net> (http://stephane.shimaore.net/)",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/shimaore/royal-thing/issues"
  },
  "homepage": "https://github.com/shimaore/royal-thing",
  "devDependencies": {
    "chai": "^1.10.0",
    "chai-as-promised": "^4.1.1",
    "coffee-script": "^1.8.0",
    "exec-as-promised": "^1.0.1",
    "mocha": "^2.1.0",
    "superagent-as-promised": "^2.0.0"
  },
  "dependencies": {
    "bluebird": "^2.9.10",
    "exec-as-promised": "^1.1.0",
    "opensips": "^1.1.3",
    "pouchdb": "^3.3.1",
    "winston": "^0.9.0"
  }
}
